{
  "redirects": [
    {
      "source": "/(.*)", 
      "has": [{ "type": "host", "value": "thedinnerdecider.au" }], 
      "destination": "https://www.thedinnerdecider.au/$1", 
      "permanent": true 
    }
  ],

  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

